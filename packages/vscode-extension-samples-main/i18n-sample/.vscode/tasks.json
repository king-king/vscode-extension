{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"isBackground": true,
			"problemMatcher": [
				"$tsc-watch"
			]
		},
		{
			"type": "npm",
			"script": "compile",
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"type": "gulp",
			"task": "clean",
			"problemMatcher": []
		},
		{
			"type": "gulp",
			"task": "build",
			"problemMatcher": [
				"$tsc"
			]
		}
	]
}